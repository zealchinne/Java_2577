class Testarray3{  
static void max(int arr[]) //creating a method which receives an array as a parameter  
	{  
	int max=arr[0];  
	for(int i=1;i<arr.length;i++)  
	if(max<arr[i])  
	max=arr[i];  
	System.out.println(max);    
	}
public static void main(String args[]){  
int a[]={103,23,65,95};//declaring and initializing an array  
max(a);//passing array to method  
}}  
